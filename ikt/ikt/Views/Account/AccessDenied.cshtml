@{
    Layout = null;
    ViewBag.Title = "Åtkomst nekad";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - @Constants.SiteName</title>
    <link href="https://fonts.googleapis.com/css?family=Poppins:400" rel="stylesheet">
    <script defer src="https://use.fontawesome.com/releases/v5.0.8/js/all.js"></script>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container jumbotron">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <h1 style="text-align:center;">@ViewBag.Title</h1>
            </div>
        </div>
        <hr style="border-top: 1px solid black" />
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <p>
                    Du har inte rättigheter för att komma åt denna applikation. Kontakta systemadministratören om du har frågor.
                </p>
                <p>
                    Du bör stänga alla fönster i webbläsaren.
                </p>
            </div>
        </div>
    </div>
    @Html.Partial("_FooterPartial")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
